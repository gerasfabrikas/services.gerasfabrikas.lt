<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="GFBundle\Entity\Task" table="task">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="phid" type="string" column="phid" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="author" type="string" column="author" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="owner" type="string" column="owner" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="string" column="status" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="integer" column="title" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="description" type="integer" column="description" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="datecreated" type="integer" column="dateCreated" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="datemodified" type="integer" column="dateModified" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
