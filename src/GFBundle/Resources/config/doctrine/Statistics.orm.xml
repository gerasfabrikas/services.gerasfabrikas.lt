<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="GFBundle\Entity\Statistics" table="statistics">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="task" type="string" column="task" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="project" type="string" column="project" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="user" type="string" column="user" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="hours" type="integer" column="hours" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="category" type="string" column="category" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
